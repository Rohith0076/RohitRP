package Programms;

import java.util.Arrays;

public class Median {
	
	public static void main(String[] args) {
		
		int[] i= {1,2,3};
		int[] j= {4,5,6};
		int x=i.length+j.length;
		int[] m=new int[];
	
		int n=0;
		for (int k = 0; k < i.length; k++) {
			 int l=i[k];
			for (int z = 0; z < j.length; z++) {
				if(l>=j[z]) {
					m[n++]=j[z];
				}else {
					m[n++]=l;
				}
			}
			
		}
		System.out.println(Arrays.toString(m));
	}

}
